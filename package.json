    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chai": "^4.3.4",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^27.3.1",
    "mocha": "^9.1.3",
    "nyc": "~15.1.0",
    "rimraf": "~3.0.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
